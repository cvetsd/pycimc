<!--storageVirtualDriveCreatorUsingUnusedPhysicalDrive-->
        <xs:element name="storageVirtualDriveCreatorUsingUnusedPhysicalDrive" type="storageVirtualDriveCreatorUsingUnusedPhysicalDrive" substitutionGroup="managedObject"/>
        <xs:complexType name="storageVirtualDriveCreatorUsingUnusedPhysicalDrive" mixed="true">
            <xs:annotation>
                <xs:appinfo>
                    <moClass value="storageVirtualDriveCreatorUsingUnusedPhysicalDrive"/>
                    <label value="Create new virtual drive from unused physical drives."/>
                    <rn value="virtual-drive-create"/>
                    <access value="admin"/>
                </xs:appinfo>
            </xs:annotation>

            <xs:attribute name="virtualDriveName">
                <xs:simpleType>
                    <xs:restriction base="xs:string">
                         <xs:minLength value="0"/>
                         <xs:maxLength value="15"/>
                    </xs:restriction>
                </xs:simpleType>
            </xs:attribute>

            <xs:attribute name="size">
                <xs:simpleType>
                    <xs:restriction base="xs:string">
                         <xs:minLength value="1"/>
                         <xs:maxLength value="20"/>
                         <xs:pattern value="(\d+\s?([MGT]B)?)"/>
                    </xs:restriction>
                </xs:simpleType>
            </xs:attribute>

            <xs:attribute name="raidLevel">

                <!-- raidLevel default = 0 -->
                <!-- Cisco IMC Supports the following RAID level for Virtual Drive creation  -->

                <xs:simpleType>
                    <xs:restriction base="xs:string">
                         <xs:enumeration value="0"/>
                         <xs:enumeration value="1"/>
                         <xs:enumeration value="5"/>
                         <xs:enumeration value="6"/>
                         <xs:enumeration value="10"/>
                         <xs:enumeration value="50"/>
                         <xs:enumeration value="60"/>
                    </xs:restriction>
                </xs:simpleType>
            </xs:attribute>

            <!-- Minimum physical drives required for different RAID level are:  
                    RAID 0 at least 1 physical drive, Single Span only
                    RAID 1 at least 2 physical drives, Single Span only 
                    RAID 5 at least 3 physical drives, Single Span only 
                    RAID 6 at least 3 physical drives, Single Span only
                    RAID 10 at least 4 physical drives, Min 2 Spans Max 8 Spans 
                    RAID 50 at least 6 physical drives, Min 2 Spans Max 8 Spans
                    RAID 60 at least 6 physical drives, Min 2 Spans Max 8 Spans  
            -->

            <xs:attribute name="driveGroup" use="required">

                <xs:simpleType>
                    <xs:restriction base="xs:string">
                         <xs:minLength value="1"/>
                         <xs:maxLength value="512"/>
                         <xs:pattern value="((\[\d+(,\d+)*\])(\[\d+(,\d+)*\])*)"/>
                    </xs:restriction>
                </xs:simpleType>

                <!-- E.g. for RAID 1, Single Span with 2 unused physical drives, 
                     driveGroup = "[5,19]"  
                -->

                <!-- E.g. for RAID 50, 3 Spans each with 3 unused physical drives, 
                     driveGroup = "[1,2,3][4,5,11][6,7,9]"  
                -->
            </xs:attribute>

            <xs:attribute name="writePolicy">

                <!-- writePolicy default = "Write Back Good BBU" -->

                <xs:simpleType>
                    <xs:restriction base="xs:string">
                         <xs:enumeration value="Write Through"/>
                         <xs:enumeration value="Write Back Good BBU"/>
                         <xs:enumeration value="Always Write Back"/>
                         <xs:enumeration value="write-through"/>
                         <xs:enumeration value="write-back-good-bbu"/>
                         <xs:enumeration value="always-write-back"/>
                         <xs:enumeration value="default"/>
                    </xs:restriction>
                </xs:simpleType>
            </xs:attribute>

            <xs:attribute name="stripSize" >
                <xs:simpleType>
                    <xs:restriction base="xs:string">
                         <xs:enumeration value="8k"/>
                         <xs:enumeration value="16k"/>
                         <xs:enumeration value="32k"/>
                         <xs:enumeration value="64k"/>
                         <xs:enumeration value="128k"/>
                         <xs:enumeration value="256k"/>
                         <xs:enumeration value="512k"/>
                         <xs:enumeration value="1024k"/>
                         <xs:enumeration value="default"/>
                    </xs:restriction>
                </xs:simpleType>
            </xs:attribute>

            <xs:attribute name="readPolicy">
                <xs:simpleType>
                    <xs:restriction base="xs:string">
                         <xs:enumeration value="no-read-ahead"/>
                         <xs:enumeration value="always-read-ahead"/>
                         <xs:enumeration value="default"/>
                    </xs:restriction>
                </xs:simpleType>
            </xs:attribute>

            <xs:attribute name="cachePolicy">
                <xs:simpleType>
                    <xs:restriction base="xs:string">
                         <xs:enumeration value="direct-io"/>
                         <xs:enumeration value="cached-io"/>
                         <xs:enumeration value="default"/>
                    </xs:restriction>
                </xs:simpleType>
            </xs:attribute>

            <xs:attribute name="accessPolicy">
                <xs:simpleType>
                    <xs:restriction base="xs:string">
                         <xs:enumeration value="read-write"/>
                         <xs:enumeration value="read-only"/>
                         <xs:enumeration value="blocked"/>
                         <xs:enumeration value="default"/>
                    </xs:restriction>
                </xs:simpleType>
            </xs:attribute>

            <xs:attribute name="diskCachePolicy">
                <xs:simpleType>
                    <xs:restriction base="xs:string">
                         <xs:enumeration value="unchanged"/>
                         <xs:enumeration value="enabled"/>
                         <xs:enumeration value="disabled"/>
                         <xs:enumeration value="default"/>
                    </xs:restriction>
                </xs:simpleType>
            </xs:attribute>

            <xs:attribute name="adminAction">
                <xs:simpleType>
                    <xs:restriction base="xs:string">
                         <xs:enumeration value="enable-self-encrypt"/>
                    </xs:restriction>
                </xs:simpleType>
            </xs:attribute>

            <xs:attribute name="adminState" use="required">
                <xs:simpleType>
                    <xs:restriction base="xs:string">
                         <xs:enumeration value="trigger"/>
                    </xs:restriction>
                </xs:simpleType>
            </xs:attribute>

            <xs:attribute name="dn" type="referenceObject"/>
            <xs:attribute name="rn" type="referenceRn"/>
            <xs:attribute name="status" type="objectStatus"/>
        </xs:complexType>

